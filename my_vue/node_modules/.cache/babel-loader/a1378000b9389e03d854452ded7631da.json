l: {{ info.email }} <br />\r\n              Password: {{ info.password }} <br />\r\n              Username: {{ info.username }} <br />\r\n              Age: {{ info.age }} <br />\r\n              Gender: {{ info.Gender }} <br />\r\n            </li>\r\n          </ul>\r\n          <!-- Button trigger modal -->\r\n          <button\r\n            type=\"button\"\r\n            class=\"btn btn-primary\"\r\n            data-bs-toggle=\"modal\"\r\n            data-bs-target=\"#manGender-profile\"\r\n          >\r\n            Manange profile\r\n          </button>\r\n          <div\r\n            class=\"modal fade\"\r\n            id=\"manGender-profile\"\r\n            tabindex=\"-1\"\r\n            aria-labelledby=\"myModalLabel\"\r\n            aria-hidden=\"true\"\r\n          >\r\n            <div class=\"modal-dialog\">\r\n              <div class=\"modal-content\">\r\n                <div class=\"modal-header\">\r\n                  <h5 class=\"modal-title\" id=\"myModalLabel\">\r\n                    ManGender profile\r\n                  </h5>\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn-close\"\r\n                    data-bs-dismiss=\"modal\"\r\n                    aria-label=\"Close\"\r\n                  ></button>\r\n                </div>\r\n                <div class=\"modal-body\">\r\n                  <form class=\"row g-3\">\r\n                    <div class=\"col-md-12\">\r\n                      <label class=\"form-label\" for=\"inputPhoto\">Photo</label>\r\n                      <input type=\"file\" class=\"form-control\" id=\"inputPhoto\" />\r\n                    </div>\r\n                    <div class=\"col-md-12\">\r\n                      <label for=\"inputEmail4\" class=\"form-label\">Email</label>\r\n                      <input\r\n                        type=\"email\"\r\n                        class=\"form-control\"\r\n                        id=\"inputEmail4\"\r\n                        v-model=\"profileFormData.email\"\r\n                        disabled\r\n                      />\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                      <label for=\"inputUsername\" class=\"form-label\"\r\n                        >Username</label\r\n                      >\r\n                      <input\r\n                        type=\"text\"\r\n                        class=\"form-control\"\r\n                        id=\"inputUsername\"\r\n                        v-model=\"profileFormData.username\"\r\n                        maxlength=\"16\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"col-md-6\">\r\n                      <label for=\"inputPassword4\" class=\"form-label\"\r\n                        >Password</label\r\n                      >\r\n                      <input\r\n                        type=\"password\"\r\n                        class=\"form-control\"\r\n                        id=\"inputPassword4\"\r\n                        v-model=\"profileFormData.password\"\r\n                        maxlength=\"16\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"col-md-6\">\r\n                      <label for=\"inputDOB\" class=\"form-label\">age</label>\r\n                      <input\r\n                        type=\"date\"\r\n                        class=\"form-control\"\r\n                        id=\"inputDOB\"\r\n                        v-model=\"profileFormData.age\"\r\n                      />\r\n                    </div>\r\n                    <div class=\"col-md-6\">\r\n                      <label for=\"inputGender\" class=\"form-label\">Gender</label>\r\n                      <select\r\n                        id=\"inputGender\"\r\n                        class=\"form-select\"\r\n                        v-model=\"profileFormData.Gender\"\r\n                      >\r\n                        <option selected>Please select</option>\r\n                        <option>Male</option>\r\n                        <option>Female</option>\r\n                        <option>Others</option>\r\n                      </select>\r\n                    </div>\r\n                    <div class=\"col-12\">\r\n                      <label for=\"introduction\" class=\"form-label\"\r\n                        >Introduction</label\r\n                      >\r\n                      <textarea\r\n                        id=\"introduction\"\r\n                        class=\"form-control\"\r\n                        rows=\"3\"\r\n                        maxlength=\"350\"\r\n                        v-model=\"profileFormData.intro\"\r\n                      ></textarea>\r\n                    </div>\r\n                  </form>\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                  <button\r\n                    type=\"button\"\r\n                    class=\"btn btn-secondary\"\r\n                    data-bs-dismiss=\"modal\"\r\n                  >\r\n                    Close\r\n                  </button>\r\n                  <button\r\n                    type=\"submit\"\r\n                    class=\"btn btn-primary\"\r\n                    @click=\"updateInfo()\"\r\n                  >\r\n                    Update\r\n                  </button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"ProfileBody\",\r\n  data() {\r\n    return {\r\n      infoList: [\r\n        {\r\n          _id: \"\",\r\n          email: \"\",\r\n          avatar: \"\",\r\n          password: \"\",\r\n          username: \"\",\r\n          age: \"\",\r\n          Gender: \"\",\r\n          intro: \"\",\r\n        },\r\n      ],\r\n      profileFormData: {\r\n        _id: \"\",\r\n        email: \"\",\r\