ml\" target=\"_blank\">\r\n                    <img\r\n                      src=\"resources/ZCC.JPG\"\r\n                      class=\"card-img-top\"\r\n                      alt=\"...\"\r\n                    />\r\n                  </a>\r\n                  <div class=\"card-body\">\r\n                    <h5>{{ video.title }}</h5>\r\n                    <p>{{ video.author }}</p>\r\n                    <p>{{ video.date }}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div\r\n          class=\"tab-pane fade\"\r\n          id=\"favourite-tab-pane\"\r\n          role=\"tabpanel\"\r\n          aria-labelledby=\"favourite-tab\"\r\n          tabindex=\"0\"\r\n        >\r\n          <div class=\"favourite-videos-area\">\r\n            <div class=\"row row-cols-4\">\r\n              <div\r\n                class=\"col\"\r\n                v-for=\"video in favouriteVideoList\"\r\n                :key=\"video._id\"\r\n              >\r\n                <div class=\"card\">\r\n                  <a href=\"blankpGender.html\" target=\"_blank\">\r\n                    <img\r\n                      src=\"resources/ZCC.JPG\"\r\n                      class=\"card-img-top\"\r\n                      alt=\"...\"\r\n                    />\r\n                  </a>\r\n                  <div class=\"card-body\">\r\n                    <h5>{{ video.title }}</h5>\r\n                    <p>{{ video.author }}</p>\r\n                    <p>{{ video.date }}</p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div\r\n          class=\"tab-pane fade\"\r\n          id=\"intro-tab-pane\"\r\n          role=\"tabpanel\"\r\n          aria-labelledby=\"intro-tab\"\r\n          tabindex=\"0\"\r\n        >\r\n          <ul type=\"none\">\r\n            <li\r\n              class=\"profile-info\"\r